{
  "name": "paicku",
  "description": "A CLI for containerizing applications with buildpacks",
  "version": "0.0.5",
  "author": "Red Hat, Inc.",
  "bin": {
    "paicku": "bin/run.js"
  },
  "bugs": "https://github.com/nodeshift/paicku/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "adm-zip": "^0.5.16",
    "lookpath": "^1.2.3",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/adm-zip": "^0.5.7",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nodeshift/paicku",
  "keywords": [
    "oclif",
    "buildpack",
    "container",
    "pack",
    "paicku",
    "docker",
    "containerization",
    "podman",
    "testcontainer"
  ],
  "license": "Apache-2.0",
  "type": "module",
  "oclif": {
    "bin": "paicku",
    "dirname": "paicku",
    "commands": "./dist/commands",
    "hooks": {
      "finally": [
        "./src/hooks/finally/clean-cloned-repo"
      ],
      "prerun": "./dist/hooks/prerun/download-pack"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodeshift/paicku.git"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme --multi",
    "test": "npm run unit:test && npm run integration:test",
    "integration:test": "mocha --forbid-only \"test/integration/**/*.test.ts\"",
    "integration:test:debug": "mocha \"test/integration/**/*.test.ts\"",
    "unit:test": "mocha --forbid-only \"test/unit/**/*.test.ts\"",
    "unit:test:debug": "mocha --timeout 0 \"test/unit/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
